function printfsr {
    printer=$2
    file=$1
    if (( -z $2 ))
	then printer='d116_sw'
    fi
    read 'answer?Are you sure you want to print <'$file'> on <'$printer'> ? (y/n)'
    if (( $answer == 'y' )) 
	then echo 'Ok, printing'
        cat $file #| ssh 8herrman@rzssh1.informatik.uni-hamburg.de lpr -U fsr -P$printer -o sides=one-sided
    else
        echo 'Ok, not printing'
    fi
}
